#f the item in the list is present in the dic -- print the item in the dict
else print : address is not present in D

lis=["name","age","gender","address"]
dic={"name":"cat","age":2,"gender":'f'}
for i in lis:
    if i  in dic.keys():
        pass
print(dic.items())

#eliminate duplicate elements in a lisyt
lis=[1,2,2,3,4,4,5,6,6,1]
new_lis=[]
for i in lis:

    if len(new_lis)==0 or i!=new_lis[-1]:

        new_lis.append(i)
print(new_lis)


